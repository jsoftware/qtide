#!/bin/sh
set -e

S=$(dirname "$0")
UNAME=$(uname | tr '[:upper:]' '[:lower:]')
CPU=$(uname -m)


if [ "$UNAME" = "darwin" ] ; then
if [ "$CPU" = "arm64" ] ; then
CPU=aarch64
fi
rm -rf $S/bin/mac-$CPU
rm -rf $S/lib/build/mac-$CPU
rm -rf $S/main/build/mac-$CPU
else
if [ "$CPU" = "arm64" ] ; then
CPU=aarch64
elif [ "$CPU" = "amd64" ] ; then
CPU=x86_64
fi
rm -rf $S/bin/$UNAME-$CPU
rm -rf $S/lib/build/$UNAME-$CPU
rm -rf $S/main/build/$UNAME-$CPU
fi
